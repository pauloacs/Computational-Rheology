transportModel  Newtonian;

nu              [0 2 -1 0 0 0 0] 1;
nu_p		[0 2 -1 0 0 0 0] 1;  //viscosidade dinâmica do polímero / rho_solvente
nu_stab		[0 2 -1 0 0 0 0] 50;
lambda		[0 0  1 0 0 0 0] 0.333; //tempo de relaxação
